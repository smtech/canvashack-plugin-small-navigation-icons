<?xml version="1.0"?>
<canvashack>
    <name>Small Navigation Icons</name>
    <id>small-navigation-icons</id>
    <abstract>Show global navigation icons at a smaller size</abstract>
    <authors>
        <author>
            <name>Seth Battis</name>
            <email>SethBattis@stmarksschool.org</email>
        </author>
    </authors>
    <components>
        <css>small-navigation-icons.css</css>
        <canvas>
            <pages>
                <include>
                    <page>
                        <type>regex</type>
                        <pattern>/.*/</pattern>
                    </page>
                </include>
            </pages>
        </canvas>
    </components>
</canvashack>
